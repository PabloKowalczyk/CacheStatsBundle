<phpunit
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/8.5/phpunit.xsd"
    verbose="true"
    cacheResult="false"
    colors="true"
>
    <testsuites>
        <testsuite name="Unit test">
            <directory>./tests/Unit/*</directory>
        </testsuite>
    </testsuites>
</phpunit>
